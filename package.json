{
  "name": "cordova-res",
  "version": "0.15.4",
  "description": "This tool will crop and resize PNG source images into appropriate sizes for modern iOS and Android devices.",
  "homepage": "https://ionicframework.com",
  "author": "Ionic Team <hi@ionicframework.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.3.0"
  },
  "bin": {
    "cordova-res": "./bin/cordova-res"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --maxWorkers=4",
    "lint": "npm run eslint && npm run prettier -- --check",
    "fmt": "npm run prettier -- --write",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier 'src/**/*.ts'",
    "release": "np",
    "prepublishOnly": "npm run build"
  },
  "prettier": "@ionic/prettier-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/cordova-res.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/cordova-res/issues"
  },
  "keywords": [
    "ionic",
    "cordova",
    "phonegap",
    "resources",
    "mobile",
    "native",
    "icon",
    "splash screen"
  ],
  "dependencies": {
    "@ionic/utils-array": "2.1.6",
    "@ionic/utils-fs": "3.1.7",
    "debug": "4.3.4",
    "elementtree": "^0.1.7",
    "sharp": "0.32.6",
    "tslib": "2.6.2"
  },
  "devDependencies": {
    "@ionic/eslint-config": "0.3.0",
    "@ionic/prettier-config": "4.0.0",
    "@types/debug": "4.1.8",
    "@types/elementtree": "0.1.0",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.5.2",
    "@types/node": "14.18.63",
    "@types/sharp": "0.32.0",
    "eslint": "7.32.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "np": "7.7.0",
    "prettier": "3.0.3",
    "ts-jest": "27.1.5",
    "typescript": "3.9.10"
  }
}
